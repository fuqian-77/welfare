<!--450*450-->
<chart caption='%title1%' subcaption='%title2%' showPercentValues='0' decimals='1' baseFontSize='11' isSliced='0'>
   <set label="%label%" value="%value%" />
   <styles>
      <definition>
         <style type='font' name='captionFont' size='15' />
         <style type='font' name='subcaptionFont' size='13' />
      </definition>
      <application>
      <apply toObject='CAPTION' styles='captionFont' />
      <apply toObject='SUBCAPTION' styles='subcaptionFont' />
      </application>
   </styles>
</chart>