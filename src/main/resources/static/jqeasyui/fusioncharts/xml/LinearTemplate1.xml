<Chart caption="%title1%" bgColor="000000" bgAlpha="0" showBorder="0" upperLimit="%upperlimit%" lowerLimit="%lowerlimit%" gaugeRoundRadius="5" chartBottomMargin="50" ticksBelowGauge="0" 
showGaugeLabels="0" valueAbovePointer="0" pointerOnTop="1" pointerRadius="9">
    <colorRange> 
        <color minValue="%lowerlimit%" maxValue="%colorrange1%" name="GOOD"  />
        <color minValue="%colorrange1%" maxValue="%colorrange2%" name="WEAK" /> 
        <color minValue="%colorrange2%" maxValue="%upperlimit%" name="BAD" />
    </colorRange>
    <value>%value1%</value>
  <annotations>
    <annotationGroup xPos='135' yPos='180' showBelow='1'>		
      <annotation type="text" label="%title1%" fontColor="FFFFFF" fontSize="12" isBold="0"/>
    </annotationGroup>
    <annotationGroup xPos='135' yPos='195' showBelow='1'>		
      <annotation type="text" label="%title2%" fontColor="FFFFFF" fontSize="12" isBold="0"/>
    </annotationGroup>
  </annotations>
   <styles>
       <definition>
           <style name="ValueFont" type="Font" bgColor="333333" size="10" color="FFFFFF"/>
       </definition>
       <application>
           <apply toObject="VALUE" styles="valueFont"/>
           <apply toObject="Caption" fontcolor="000000" styles="valueFont"/>
       </application>	
   </styles>
</Chart>